void main() {
  List<int> numbers = [5, 4, 20, 10, 8, 6, 2];

  for (int i = 1; i < numbers.length; i++) {
    int temp = numbers[i];
    int j = i - 1;

    while (j >= 0 && temp < numbers[j]) {
      numbers[j + 1] = numbers[j];
      j--;
    }

    numbers[j + 1] = temp;

    print(numbers);
  }
}
